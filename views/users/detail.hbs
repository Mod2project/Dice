<div class="row justify-content-center">
  <div class="col-12 col-md-3 mb-2">
    <div class="card">
      <img src={{user.avatar}} class="card-img-top" alt={{user.name}}>
      <div class="card-body">
        <h5 class="card-title">{{user.name}}</h5>
        <form action="/logout" method="GET">
        <button type="submit" class="btn btn-outline-warning mx-0.8 mb-3">Log out</button>
        </form>
      </div>
    </div>
  </div>
 </div>

 <div>
    <h5>Next events</h5>
 </div>
   <div class="row justify-content-center pt-3 ps-5 pe-5 ">
  {{#each user.tickets as |ticket|}}
    <div class="col-12 col-md-3 mb-2">
      <div class="card">
          <img src="{{ticket.event.poster}}" class="card-img-top" alt="{{ticket.event.name}}">
        <div class="card-body">
          <h5 class="card-title">{{ticket.event.name}}</h5>
        </div>
      </div>
    </div>
  {{/each}}
